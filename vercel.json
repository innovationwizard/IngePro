{
  "rewrites": [
    {
      "source": "/:tenant/dashboard/:path*",
      "destination": "/dashboard/:path*"
    },
    {
      "source": "/:tenant/api/:path*", 
      "destination": "/api/:path*"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/rate-limit-cleanup",
      "schedule": "0 0 * * *"
    }
  ]
}